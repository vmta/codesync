# Set forth the umkoin...
# General cases, nothing to preserve
s/БІТКО/УМКО/g; s/Бітко/Умко/g; s/бітко/умко/g
s/БИТКО/УМКО/g; s/Битко/Умко/g; s/битко/умко/g
s/BITCOIN/UMKOIN/g
s/BTC/UMK/g; s/btc/umk/g

# Change link to Umkoin-Core project on Transifex
/\[Transifex\]/ s/projects\/p\/bitcoin/umkoin\/umkoin-core/

# Remove several lines concerning security and
# update notifications
/To receive security and update notifications/,+3d

# Change Bitcoin to Umkoin, but preserve
# - Bitcoin Core devel
/Bitcoin Core devel/! s/Bitcoin/Umkoin/g

# Change bitcoin to umkoin, but preserve
# - bitcoin-core
# - bitcoincore
# - bitcoin.stackexchange
# - bitcointalk
# - bitcoin/bips
# - doxygen
# - en.bitcoin.it
/bitcoin-core/!{
/bitcoincore/!{
/bitcoin.stackexchange/!{
/bitcointalk/!{
/bitcoin\/bips/!{
/doxygen/!{
/en.bitcoin.it/! s/bitcoin/umkoin/g}}}}}}

# Change ports 8332 to 6332, 8333 to 6333, 18333 to 16333
# but preserve any other combination of alphanumeric
# sequences with angle brackets
/\<833[23]\>/ y/8/6/

# Change tor onion address
s/5wyqrzbvrdsumnok/klfchu53kxun6zx5/g

# Change the initial copyright date from 2009 to 2017
s/arg(2009)/arg(2017)/g
/COPYRIGHT_YEAR/ s/2009/2017/g

# Set back the bitcoin...
#s/en.umkoin.it/en.bitcoin.it/g

s/umkoin\/umkoin\/blob\//bitcoin\/bitcoin\/blob\//g
s/umkoin\/umkoin\/issues\//bitcoin\/bitcoin\/issues\//g
s/umkoin\/umkoin\/pull\//bitcoin\/bitcoin\/pull\//g

# Set forth the umkoin in special cases...

# Change bitcoin-core to umkoin provided that it is used
# in conjunction with the github.com, but preserve
# - secp256k1
# - ctaes
# - univalue
/github.com/{/secp256k1/!{/ctaes/!{/univalue/! s/bitcoin-core/umkoin/g}}}

# Change bitcoincore to www.umkoin provided that it is used
# in conjunction with
# - download link, and change the link to download.php as well
# - cfields link
# - *version*
# - AC_INIT
# - URL_WEBSITE
/\/download\// s/bitcoincore/www.umkoin/g
/\/download\// s/\/download\//\/download.php/g
/\/cfields\// s/bitcoincore/www.umkoin/g
/\*version\*/ s/bitcoincore/www.umkoin/
/\*version\*/ s/bitcoin-core/umkoin-core/
/AC_INIT/ s/bitcoincore/www.umkoin/g
/URL_WEBSITE/ s/bitcoincore/www.umkoin/g

#s/github.com\/bitcoin-core\/gitian.sigs/github.com\/umkoin\/gitian.sigs/g
#s/github.com\/bitcoin-core\/umkoin/github.com\/umkoin\/umkoin/g

